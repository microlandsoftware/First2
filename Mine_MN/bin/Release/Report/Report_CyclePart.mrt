      <show_servicelot Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>show_servicelot</Alias>
        <Columns isList="true" count="6">
          <value>namelot,System.String</value>
          <value>typeactivity,System.String</value>
          <value>grouprepair,System.String</value>
          <value>date,System.String</value>
          <value>time,System.String</value>
          <value>status,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>show_servicelot</Name>
        <NameInSource>connect</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>if ('{Type}'='all')
select MN_Part.Name as namelot,MN_FixType.type as typeactivity,MN_Group.type as grouprepair,dbo.int_tostringdate(convert(int,MN_CreatePartFix.CreateDate)) as date,convert(varchar(10),MN_CreatePartFix.CreateTime) as time,CASE MN_CreatePartFix.Confirm  WHEN 0 THEN N'تائید نشده' ELSE N'تائید شده' end as status    from MN_CreatePartFix join
MN_DefinePartMode on MN_CreatePartFix.PartModeID=MN_DefinePartMode.ID  
join MN_Group on MN_DefinePartMode.GroupID=MN_Group.ID
join MN_FixType on MN_DefinePartMode.FixTypeID=MN_FixType.ID 
join MN_Part on MN_DefinePartMode.PartID=MN_Part.ID where MN_DefinePartMode.VehicleID={DeviceCode} and  MN_CreatePartFix.CreateDate between {StartDate} and {EndDate}

if ('{Type}'='repair')
select MN_Part.Name as namelot,MN_FixType.type as typeactivity,MN_Group.type as grouprepair,dbo.int_tostringdate(convert(int,MN_CreatePartFix.CreateDate)) as date,convert(varchar(10),MN_CreatePartFix.CreateTime) as time,CASE MN_CreatePartFix.Confirm  WHEN 0 THEN N'تائید نشده' ELSE N'تائید شده' end as status    from MN_CreatePartFix join
MN_DefinePartMode on MN_CreatePartFix.PartModeID=MN_DefinePartMode.ID  
join MN_Group on MN_DefinePartMode.GroupID=MN_Group.ID
join MN_FixType on MN_DefinePartMode.FixTypeID=MN_FixType.ID 
join MN_Part on MN_DefinePartMode.PartID=MN_Part.ID where MN_DefinePartMode.VehicleID={DeviceCode} and  MN_CreatePartFix.CreateDate between {StartDate} and {EndDate} and MN_FixType.ID=2

if ('{Type}'='xchange')
select MN_Part.Name as namelot,MN_FixType.type as typeactivity,MN_Group.type as grouprepair,dbo.int_tostringdate(convert(int,MN_CreatePartFix.CreateDate)) as date,convert(varchar(10),MN_CreatePartFix.CreateTime) as time,CASE MN_CreatePartFix.Confirm  WHEN 0 THEN N'تائید نشده' ELSE N'تائید شده' end as status    from MN_CreatePartFix join
MN_DefinePartMode on MN_CreatePartFix.PartModeID=MN_DefinePartMode.ID  
join MN_Group on MN_DefinePartMode.GroupID=MN_Group.ID
join MN_FixType on MN_DefinePartMode.FixTypeID=MN_FixType.ID 
join MN_Part on MN_DefinePartMode.PartID=MN_Part.ID where MN_DefinePartMode.VehicleID={DeviceCode} and  MN_CreatePartFix.CreateDate between {StartDate} and {EndDate} and MN_FixType.ID=1
</SqlCommand>
      </show_servicelot>
